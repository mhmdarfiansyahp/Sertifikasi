<body>
    <header>
        <div class="polman-nav-static-top" style="display: flex; align-items: center; justify-content: space-between;">
            <div class="float-left">
                <a asp-controller="Login" asp-action="Index">
                    <img src="~/Content/Images/IMG_1687.png" style="height: 50px;" />
                </a>
            </div>

            <div class="dropdown" id="login" style="padding-right: 2%;">
                <div class="col-lg-4 mt-2 mb-2">
                    <a class="btn btn-primary" asp-controller="Login" asp-action="Index">Masuk</a>
                </div>
            </div>
        </div>
    </header>
		<!-- End Google Tag Manager (noscript) -->

		@*<nav class="navbar navbar-expand-lg navbar-light light">
            <a class="navbar-brand" style="padding: 5px 15px;" asp-controller="Login" asp-action="Index">
