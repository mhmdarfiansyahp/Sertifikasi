			</a>
		</nav>*@
		<div class="container-fluid" style="padding-top: 50px;">
			<!-- Adjust the value according to your needs -->
			@RenderBody()
		</div>

        <style>
        .polman-nav-static-top {
            top: 0;
            left: 0;
            right: 0;
            position: fixed;
            height: 70px;
            width: 100% !important;
            padding: 10px 20px 10px 20px;
            box-shadow: 0px 2px 0px 0px #EEE;
            background-color: white;
            z-index: 4;
        }
        </style>
    
</body>
</html>

